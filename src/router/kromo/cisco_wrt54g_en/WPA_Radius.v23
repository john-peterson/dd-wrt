<div class="setting">
	<div class="label"><script type="text/javascript">Capture(wpa.algorithms)</script></div>
	<select name="wl_crypto">
		<option value="tkip" <% nvram_selmatch("wl_crypto","tkip","selected"); %>>TKIP</option>
		<option value="aes" <% nvram_selmatch("wl_crypto","aes","selected"); %>>AES</option>
		<option value="tkip+aes" <% nvram_selmatch("wl_crypto","tkip+aes","selected"); %>>TKIP+AES</option>
	</select>
</div>
<div class="setting">
	<div class="label"><script type="text/javascript">Capture(wpa.radius_ipaddr)</script></div>
	<input type="hidden" name="wl_radius_ipaddr" value="4" />
	<input size="3" maxlength="3" name="wl_radius_ipaddr_0" onblur=valid_range(this,0,255,'IP') class="num" value='<% get_single_ip("wl_radius_ipaddr","0"); %>' />.<input size="3" maxlength="3" name="wl_radius_ipaddr_1" onblur="valid_range(this,0,255,'IP')" class="num" value="<% get_single_ip("wl_radius_ipaddr","1"); %>" />.<input size="3" maxlength="3" name="wl_radius_ipaddr_2" onblur="valid_range(this,0,255,'IP')" class="num" value="<% get_single_ip("wl_radius_ipaddr","2"); %>" />.<input size="3" maxlength="3" name="wl_radius_ipaddr_3" onblur="valid_range(this,1,254,'IP')" class="num" value="<% get_single_ip("wl_radius_ipaddr","3"); %>" />
</div>
<div class="setting">
	<div class="label"><script type="text/javascript">Capture(wpa.radius_port)</script></div>
	<input name="wl_radius_port" size="3" maxlength="5" onblur=valid_range(this,1,65535,'Port') value='<% nvram_get("wl_radius_port"); %>' />
</div>
<div class="setting">
	<div class="label"><script type="text/javascript">Capture(wpa.shared_key)</script></div>
	<input name="wl_radius_key" size="20" maxlength="79" value='<% nvram_get("wl_radius_key"); %>' />
</div>
<div class="setting">
	<div class="label"><script type="text/javascript">Capture(wpa.rekey)</script></div>
	<input name="wl_wpa_gtk_rekey" maxlength="5" size="10" onblur="valid_range(this,0,99999,'rekey interval')" value='<% nvram_get("wl_wpa_gtk_rekey"); %>' /> seconds
</div>

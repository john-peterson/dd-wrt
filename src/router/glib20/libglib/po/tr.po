msgstr "URI '%s' geçersiz"

#: ../glib/gconvert.c:1921
#, c-format
msgid "The hostname of the URI '%s' is invalid"
msgstr "URI makine adı '%s' geçersiz"

#: ../glib/gconvert.c:1937
#, c-format

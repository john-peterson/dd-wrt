#debug stuff
logfd 2
debug
dump
#
defaultroute
lock
noauth
nodetach
#refuse-eap
refuse-pap
refuse-chap
refuse-mschap
nobsdcomp
nodeflate
#mppe required,stateless,no40,no56
mppc
lcp-echo-failure 30
lcp-echo-interval 2
persist
usepeerdns
idle 0
ip-up-script /tmp/pptpd_client/ip-up
ip-down-script /tmp/pptpd_client/ip-down
ipparam kelokepptpd
